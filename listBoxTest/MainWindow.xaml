<Window x:Class="listBoxTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:listBoxTest"
        mc:Ignorable="d"
        Title="FIle manager" Height="500" Width="450" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>


        <Button Content="Open" HorizontalAlignment="Left" Margin="359,10,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" Height="20"/>
        <Button  x:Name="backButton"  HorizontalAlignment="Left" Margin="10,35,0,0" VerticalAlignment="Top" Width="75" Height="20" RenderTransformOrigin="-2.12,3.5" Click="Button_Click_1" Content="&lt;&lt;"/>
        <Button  x:Name="forwardButton"  HorizontalAlignment="Left" Margin="90,35,0,0" VerticalAlignment="Top" Width="75" Height="20" RenderTransformOrigin="-2.12,3.5"  Content="&gt;&gt;" Click="forwardButton_Click"/>

        <TextBox x:Name="TextBox1" Text="" HorizontalAlignment="Left" Height="20" Margin="10,10,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="335"  IsReadOnly="True"/>
        <WebBrowser x:Name="webBrowser" HorizontalAlignment="Left" Height="350" Margin="10,110,10,0" VerticalAlignment="Top"  Width="430"/>


    </Grid>
</Window>
